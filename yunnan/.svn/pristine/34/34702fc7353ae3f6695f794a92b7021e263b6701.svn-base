@font34:.34rem ;
@font28:.28rem ;
@font32:.32rem ;
@font24:.24rem;
@font-color:#333;
.margin_b16{
    margin-bottom: .16rem;
}
.margin_b32{
    margin-bottom: .32rem;
}
.bor_b{
    border-bottom: solid 0.01rem #ccc;
}
.font-34{
    font-size: @font34 !important;
}
.font-24{
    font-size: @font24 !important;
}
.font-28{
    font-size: @font28 !important;
}
.col_333{
    color: @font-color;
}
.col_fb{
    color: #2D7FFC;
}
.col_999{
    color: #999999;
}
.dis_flex{
    display: flex;
}
.flex_1{
    display: block;
    flex: 1;
}
.Putonghua{
    display: flex;
    padding: 0 .36rem;
    height: 1rem;
    background: #fff;
    border-bottom: solid 0.01rem #ccc;
}
.label{
    display: block;
    width: 2.5rem;
    height: 1rem;
    line-height: 1rem;
    font-size: .28rem;
    color: #333;
    text-align: left;
}
.input{
    display: block;
    height: 1rem;
    line-height: 1rem;
    flex: 1;
    border: none;
    font-size: .28rem;
    text-align: right;
    background: none !important;
}
.btn{
  display: block;
  width: 5.9rem;
  height: .9rem;
  font-size: .32rem;
  color: #fff;
  border: none;
  border-radius: .5rem;
  margin: 0 auto;
  margin-top: 1.02rem;
  background: #2D7FFC;
}
.main{
    padding: .28rem 0 .01rem;
    background: #fff;
    position: relative;
    width: 100%;
    margin-bottom: 1rem;
}
.regional{
    width: 100%;
    margin-bottom: .5rem;
}
.regional p{
    text-align: left;
    display: flex;
}
.regional p span{
    display: block;
    color: #666666;
    font-size: .28rem;
    line-height: .6rem;
    margin: 0 .36rem;
}
.regional p span:first-child{
    width: 20%;
    margin-right: 0;
}
.img_fl{
    position: absolute;
    right: 1.8rem;
    top: 2.7rem;
    width: 1.5rem;
    height: 2.05rem;
  }
.education{
  text-align:left;
  font-size:0.28rem;
  .card{
    width: 6.78rem;
    height: 4.26rem;
    margin: 0 auto 0.16rem;
    background: url(../images/gardquery_bg.png) no-repeat left top;
    background-size: 100%;
    .card_body{
      padding: .48rem .42rem .72rem;
      .logo{
        width: 1.44rem;
        height: .28rem;
        margin-bottom: .15rem;
        background: url(../images/logo.png) no-repeat left top;
        background-size: 100%;
      }
      .bureau_name{
        font-size: @font32;
        font-weight: bold;
        margin-bottom: .53rem;
      }
      .name_id{
        width: 4.74rem;
        height: .48rem;
        line-height: .48rem;
        font-size: .26rem;
        text-align: center;
      }
      .card_id{
        margin-top: .4rem;
        font-size: .32rem;
        font-weight: bold;
      }
    }
  }
  & > div:not(.card){
    margin-bottom: 0.16rem;
    background-color: #fff;
    padding:0.4rem;
  }

  #state{
    padding: 0.3rem 0.36rem;
    overflow: hidden;
    line-height: 0.56rem;
    color:#666;
    span.img{
      float:left;
      height:0.6rem;
      width:0.6rem;
      margin-right: 0.14rem;
      background-size: 0.6rem 0.6rem;
      background-image: url("../images/renshe-card.png");
      @media (-webkit-min-device-pixel-ratio:3),(min-device-pixel-ratio:3){
        background-image: url("../images/renshe-card.png")
      }
    }
    span.symbol{
      font-size: 0.24rem;
      color:#fff;
      border-radius: 0.32rem;
      width: 0.8rem;
      height: 0.4rem;
      display: inline-block;
      line-height: 0.4rem;
      text-align: center;
      margin-left:0.16rem;
    }
    span.symbolz{
      background: #257BFF !important;
    }
    span.symbolls{
      background: #FA6366 !important;
    }
    span.symbolzk{
      background: #FAC306 !important;
    }
    span.symboly{
      background: #999 !important;
    }
    

    &>div:last-of-type{
      float:right;
      color:#999;

      span{
        vertical-align: middle;
        display: inline-block;
      }
      .arrow{
        height:0.3rem;
        width:0.18rem;
        margin-left: 0.12rem;
        background-size: 0.18rem 0.3rem;
        background-image: url("../images/arrow.png");
        @media (-webkit-min-device-pixel-ratio:3),(min-device-pixel-ratio:3){
          background-image: url("../images/arrow.png")
        }
      }
    }
  }

  .header{
    color:#333;
    font-weight: bold;
    font-size:0.3rem;
    margin-bottom: 0.44rem;

    div{
      display:inline-block;
    }
  }

  //藍色小竖条
  .blueline{
    display: inline-block;
    width: 0.06rem;
    height: 0.24rem;
    background-color: #257BFF;
    margin-right: 0.12rem;
  }

  .operation{
    display:inline-block;
    color:#666;
    margin-right:1.07rem;

    &:nth-of-type(2){
      margin-left:0.66rem;
    }
    &:last-of-type{
      margin-right: 0rem;
    }
    .img{
      margin: 0 auto 0.16rem;
      height:0.6rem;
      width:0.6rem;
      background-size: 0.6rem 0.6rem;
    }

    &#test .img{
      background-image: url("../images/renshe-card-test.png");
      @media (-webkit-min-device-pixel-ratio:3),(min-device-pixel-ratio:3){
        background-image: url("../images/renshe-card-test.png")
      }
    }
    &#intro .img{
      background-image: url("../images/renshe-card-intro.png");
      @media (-webkit-min-device-pixel-ratio:3),(min-device-pixel-ratio:3){
        background-image: url("../images/renshe-card-intro.png")
      }
    }
    &#progress .img{
      background-image: url("../images/renshe-card-progress.png");
      @media (-webkit-min-device-pixel-ratio:3),(min-device-pixel-ratio:3){
        background-image: url("../images/renshe-card-progress.png")
      }
    }
    &#record .img{
      background-image: url("../images/renshe-card-record.png");
      @media (-webkit-min-device-pixel-ratio:3),(min-device-pixel-ratio:3){
        background-image: url("../images/renshe-card-record.png")
      }
    }
  }


}
// 建筑工程招标公告查询
.bidding_main{
    width: 100%;
    height: auto;
    background: #fff;
    padding-bottom: 0.1rem;
}
.biddin_span{
    width: .96rem;
    height: .8rem;
    line-height: .8rem;
    font-size: @font28;
}
.corporate_name{
    line-height: .35rem !important;
    height: auto;
    text-align: left;
    font-size: @font28;
    color: @font-color;
}
.bidding_list{
    padding: .32rem .36rem;
}
.tx_left{
  text-align: left;
}
.tx_right{
  text-align: right;
}
/*统计局*/
.statisticalTwo{
  background: #fff;
  .divBoxOne{
    display: flex;
    height: 1rem;
    line-height: 1rem;
    text-align: center;
    p{
      flex: 1;
      text-align: center;
    }
    .retailyear{
      color: #333;
    }
    .retailabsolute{
      color: #2D7FFC;
    }
    .retailgrowth{
      color: #08C173;
    }
  }
}
.statisticalThree{
  div{
    width: 90%;
    padding: 0 .15rem;
    margin: 0 auto;
    background: #fff;
    border-radius: .2rem;
    margin-bottom: .15rem;
    h5{
      height: 1rem;
      line-height: 1rem;
      text-align: left;
      font-size: .32rem;
      font-weight: bold;
      color: #333;

      span{
        float:right
      }
    }
    p{
      display: flex;
      height: 1rem;
      line-height: 1rem;
      color: #333;
      border-bottom: solid .01rem rgb(202, 202, 202);
      span{
        display: block;
        font-size: .28rem;
      }
      .statisticalpsl{
       text-align: left;
       color: #333;
       width: 60%;
      }
      .statisticalpsr{
        flex: 1;
        text-align: right;
        color: #999;
      }
    }
    p:last-child{
      border-bottom: none;
    }
  }
}
.statisticsbtn{
  position: fixed;
  top:1.1rem;
  right:.2rem;
  width: 2rem;
  height: .6rem;
  background: #2D7FFC;
  color: #fff;
  text-align: center;
  line-height: .6rem;
  font-size: .28rem;
}

