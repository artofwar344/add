.example-select_btn-wrapper {
    margin-top: 20px;
    .hui-btn_primary.is-plain {
        height: .9rem;
        width: 5.9rem;
        line-height: .9rem;
        background: #2D7FFC;
        color: #fff;
        font-size: .32rem;
        margin: 0 auto;
        box-shadow: 0 5px 15px 0 rgba(45, 127, 252, 0.50);
        border-radius: .5rem;
    }
}

.example-select_arrowRight {
    display: block;
    width: 16px;
    height: 16px;
    background-size: contain;
    margin-left: 10px;
}

.cultrue {
    background: #F1F3F6;
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}
.cultureCen {
    .culture_box {
        height: .98rem;
        line-height: .98rem;
        background: #FFFFFF;
        padding: 0 .36rem;
        font-size: .28rem;
        color: #333333;
        text-align: left;
        border-bottom: 1px solid #F1F3F6;
        input {
            text-align: right;
            border: 0;
            float: right;
        }
        i {
            line-height: .98rem;
            float: right;
            font-size: .16rem;
        }
    }
}

.cultruecen_box{
    background: #FFFFFF;
    text-align: left;
    padding:.42rem .36rem .4rem;
    color: #333333;
    .c_b_tit{
        font-size: .28rem;
        line-height: .18rem;
        height: .18rem;
        i{
            display: block;
            float: left;
            background:#257BFF;
            width: .06rem;
            height: .24rem;
            margin-right: .18rem;
            
        }
    }
    ul{
        padding-top: .3rem;
        li{
            line-height: .7rem;
            text-align: right;
            
            .title{
               float: left;
               color: #999999;
               
            }
            .content{
               float: right;
            }
            &:after{
                content: '0';
                font-size: 0;
                display: block;
                clear: both;
                width: 100%;
                height:0px;
            }
        }
    }
    p{
        font-size: .28rem;
        line-height: .5rem;
    }
}
